  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src && stylelint src/**/*.css"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
