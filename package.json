    "@testing-library/react-hooks": "^5.1.0",
    "@testing-library/user-event": "^12.8.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.32",
    "@types/node-fetch": "^2.5.8",
    "@types/nprogress": "^0.2.0",
