  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src && stylelint src/**/*.scss",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --ext .js,.jsx,.ts,.tsx src"
    ],
    "src/**/*.{css,scss,sass}": [
      "stylelint src/**/*.scss"
    ],
    "src/**/*": [
      "prettier --write src"
    ]
  },
